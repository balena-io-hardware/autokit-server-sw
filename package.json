  "bugs": {
    "url": "https://github.com/balena-io-hardware/autokit-worker-sw/issues"
  },
  "homepage": "https://github.com/balena-io-hardware/autokit-worker-sw#readme",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/apidoc": "^0.50.0",
    "@types/express": "^4.17.14",
    "apidoc": "^0.53.1",
    "rimraf": "^3.0.2"
  },
  "apidoc": {
