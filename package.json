{
  "name": "@balena/autokit-server",
  "version": "0.0.1",
  "description": "Autokit HTTP server and web interface",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified, skipping\" && exit 0",
